modules = ["ruby-3.2", "swift-5.8"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["flutter329", "dart"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flutter Web App"

[[workflows.workflow]]
name = "Flutter Web App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "flutter run -d web-server --web-hostname=0.0.0.0 --web-port=5000 --dart-define-from-file=env.json"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80
